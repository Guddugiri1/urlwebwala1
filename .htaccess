RewriteEngine On
RewriteBase /

# Redirect index.php to homepage

RewriteCond %{THE_REQUEST} index\.php [NC]
RewriteRule ^index\.php$ https://www.urlwebwala.com/ [R=301,L]

# Redirect specific pages to homepage

RewriteCond %{THE_REQUEST} (about|career|developmentprocess|services|web_development|mobile_app_development|digital_marketing|graphics|testing|digital_card|hosting|support|portfolio|contact)\.php [NC]
RewriteRule ^(about|career|developmentprocess|services|web_development|mobile_app_development|digital_marketing|graphics|testing|digital_card|hosting|support|portfolio|contact)\.php$ https://www.urlwebwala.com/ [R=301,L]

# Handle other requests

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.php [L]
